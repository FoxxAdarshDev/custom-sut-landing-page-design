              {% if block.settings.left_content != blank %}
                <div class="content-block-content left-content">
                  {{ block.settings.left_content }}
                </div>
              {% endif %}
            </div>

            <!-- Right Column -->
            {% if block.settings.layout_type == 'two_column' %}
              <div class="content-block-column">
                <div class="content-block-header">
                  <div class="content-block-plus-icon">
                    <svg viewBox="0 0 24 24" fill="currentColor">
                      <path d="M19 13h-6v6h-2v-6H5v-2h6V5h2v6h6v2z"/>
                    </svg>
                  </div>
                  {% if block.settings.right_title != blank %}
                    <h3 class="content-block-title right-title">{{ block.settings.right_title }}</h3>
                  {% endif %}
                </div>

                {% if block.settings.right_icon_type == 'svg' and block.settings.right_svg_code != blank %}
                  <div class="content-block-icon">
                    {{ block.settings.right_svg_code }}
                  </div>
                {% elsif block.settings.right_icon_type == 'image' and block.settings.right_image != blank %}
                  <div class="content-block-image">
                    <img src="{{ block.settings.right_image | img_url: '400x300' }}" alt="{{ block.settings.right_title }}" loading="lazy">
                  </div>
                {% endif %}

                {% if block.settings.right_content != blank %}
                  <div class="content-block-content right-content">
                    {{ block.settings.right_content }}
                  </div>
                {% endif %}
              </div>
            {% endif %}
          </div>
        </div>
      </section>

    {% when 'contact_section' %}
      <section class="contact-section section-padding">
        <div class="container">
          <div class="contact-content">
            <h2 class="contact-section-title tv-section-title">{{ block.settings.contact_title | default: 'Ready for Your Next Project?' }}</h2>
            <p class="contact-subtitle">{{ block.settings.contact_subtitle | default: 'Get a custom quote for your plastic machining needs. Our experts are ready to help.' }}</p>

            {% if block.settings.contact_cta_url != blank %}
              <a href="{{ block.settings.contact_cta_url }}" class="contact-cta-button">
                {{ block.settings.contact_cta_text | default: 'Get Custom Quote' }}
              </a>
            {% endif %}
          </div>
        </div>
      </section>

  {% endcase %}
{% endfor %}

<!-- Image Popup Modal -->
<div class="image-popup-modal" id="imagePopupModal">
  <div class="image-popup-content">
    <button class="image-popup-close" onclick="closeImagePopup()">
      <svg viewBox="0 0 24 24">
        <path d="M19,6.41L17.59,5L12,10.59L6.41,5L5,6.41L10.59,12L5,17.59L6.41,19L12,13.41L17.59,19L19,17.59L13.41,12L19,6.41Z"/>
      </svg>
    </button>
    <img id="popupImage" src="" alt="">
  </div>
</div>

<script>
// Image popup functionality
function openImagePopup(imageSrc) {
  const modal = document.getElementById('imagePopupModal');
  const popupImage = document.getElementById('popupImage');

  popupImage.src = imageSrc;
  modal.classList.add('active');
  document.body.style.overflow = 'hidden';
}

function closeImagePopup() {
  const modal = document.getElementById('imagePopupModal');
  modal.classList.remove('active');
  document.body.style.overflow = 'auto';
}

// Close popup when clicking outside image
document.getElementById('imagePopupModal').addEventListener('click', function(e) {
  if (e.target === this) {
    closeImagePopup();
  }
});

// Close popup with escape key
document.addEventListener('keydown', function(e) {
  if (e.key === 'Escape') {
    closeImagePopup();
  }
});

// Auto-play videos when in viewport
const videos = document.querySelectorAll('video');
const videoObserver = new IntersectionObserver((entries) => {
  entries.forEach(entry => {
    if (entry.isIntersecting) {
      entry.target.play();
    } else {
      entry.target.pause();
    }
  });
}, { threshold: 0.5 });

videos.forEach(video => {
  videoObserver.observe(video);
});

// Enhanced animations
const observerOptions = {
  threshold: 0.1,
  rootMargin: '0px 0px -50px 0px'
};

const observer = new IntersectionObserver(function(entries) {
  entries.forEach(entry => {
    if (entry.isIntersecting) {
      entry.target.style.opacity = '1';
      entry.target.style.transform = 'translateY(0)';
    }
  });
}, observerOptions);

// Observe all animated elements
const animatedElements = document.querySelectorAll('.capability-card, .industry-item, .trust-card, .gallery-item, .certification-card');
animatedElements.forEach(el => {
  el.style.opacity = '0';
  el.style.transform = 'translateY(30px)';
  el.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
  observer.observe(el);
});

// Smooth scrolling for internal links
document.querySelectorAll('a[href^="#"]').forEach(anchor => {
  anchor.addEventListener('click', function (e) {
    e.preventDefault();
    const target = document.querySelector(this.getAttribute('href'));
    if (target) {
      target.scrollIntoView({
        behavior: 'smooth',
        block: 'start'
      });
    }
  });
});
</script>