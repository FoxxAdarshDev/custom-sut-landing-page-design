  /* Text Image Block Section - Single Card Layout */
  .text-image-block {
    background-color: var(--white);
    position: relative;
  }

  .text-image-container {
    max-width: 1400px;
    margin: 0 auto;
    padding: 0 var(--container-padding);
  }

  .text-image-single-card {
    display: flex;
    justify-content: center;
    align-items: center;
  }

  .text-image-card-content {
    background: var(--white);
    border-radius: var(--border-radius);
    box-shadow: 0 10px 40px rgba(0, 113, 185, 0.08);
    border: 1px solid var(--border-color);
    position: relative;
    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
    padding: 40px;
    max-width: 1200px;
    width: 100%;
    display: grid;
    grid-template-columns: 1fr 400px;
    gap: 40px;
    align-items: start;
  }

  /* Layout Direction Control */
  .text-image-single-card.layout-text-right .text-image-card-content {
    grid-template-columns: 400px 1fr;
  }

  .text-image-single-card.layout-text-right .text-content-section {
    order: 2;
  }

  .text-image-single-card.layout-text-right .image-content-section {
    order: 1;
  }

  .text-image-single-card.layout-text-left .text-content-section {
    order: 1;
  }

  .text-image-single-card.layout-text-left .image-content-section {
    order: 2;
  }

  .text-image-card-content::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    border-radius: var(--border-radius) var(--border-radius) 0 0;
  }

  .text-image-card-content::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    border-radius: 0 0 var(--border-radius) var(--border-radius);
    transform: scaleX(0);
    transition: transform 0.3s ease;
  }

  .text-image-card-content:hover {
    transform: translateY(-8px);
    box-shadow: 0 20px 60px rgba(0, 113, 185, 0.15);
    border-color: var(--primary-color);
  }

  .text-image-card-content:hover::after {
    transform: scaleX(1);
  }

  .text-content-section {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
  }

  .image-content-section {
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
  }

  .text-image-heading {
    font-weight: 700;
    color: var(--primary-color);
    margin-bottom: 24px;
    position: relative;
  }

  .text-image-heading::after {
    content: '';
    position: absolute;
    bottom: -8px;
    left: 0;
    width: 60px;
    height: 3px;
    background: linear-gradient(90deg, var(--primary-color), var(--accent-color));
    border-radius: 2px;
  }

  .text-image-description {
    color: var(--text-dark);
    margin: 0;
  }

  .text-image-description p {
    margin-bottom: 20px;
  }

  .text-image-description p:last-child {
    margin-bottom: 0;
  }

  .text-image-description strong {
    color: var(--primary-color);
    font-weight: 600;
  }

  /* Enhanced Bullet Points */
  .text-image-description ul {
    list-style: none;
    padding-left: 0;
    margin: 24px 0;
    background: rgba(0, 113, 185, 0.02);
    padding: 20px;
    border-radius: 12px;
    border-left: 4px solid var(--primary-color);
    transition: all 0.3s ease;
  }

  .text-image-description ul li {
    position: relative;
    padding-left: 40px;
    margin-bottom: 16px;
    transition: all 0.3s ease;
  }

  .text-image-description ul li:last-child {
    margin-bottom: 0;
  }

  .text-image-description ul li::before {
    content: '';
    position: absolute;
    left: 12px;
    top: 12px;
    width: var(--bullet-size, 8px);
    height: var(--bullet-size, 8px);
    background: var(--bullet-color, var(--primary-color));
    border-radius: 50%;
    box-shadow: 0 0 0 4px rgba(0, 113, 185, 0.2);
    transition: all 0.3s ease;
  }

  .text-image-description ul li:hover::before {
    background: var(--accent-color);
    transform: scale(1.3);
    box-shadow: 0 0 0 6px rgba(0, 113, 185, 0.3);
  }

  .text-image-visual-wrapper {
    position: relative;
    display: block;
    width: 100%;
    height: 350px;
    border-radius: var(--border-radius);
    overflow: hidden;
    box-shadow: 0 10px 30px rgba(0, 113, 185, 0.15);
    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
    background: rgba(0, 113, 185, 0.02);
  }

  .text-image-visual-wrapper:hover {
    transform: translateY(-8px);
    box-shadow: 0 20px 50px rgba(0, 113, 185, 0.25);
  }

  .text-image-img {
    width: 100%;
    height: 100%;
    object-fit: contain;
    transition: transform 0.4s ease;
    cursor: pointer;
    padding: 10px;
  }

  .text-image-visual-wrapper:hover .text-image-img {
    transform: scale(1.02);
  }

  .image-expand-btn {
    position: absolute;
    bottom: 20px;
    right: 20px;
    width: 50px;
    height: 50px;
    background: rgba(0, 113, 185, 0.9);
    border: none;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0 8px 25px rgba(0, 113, 185, 0.4);
    transition: all 0.3s ease;
    z-index: 5;
    backdrop-filter: blur(10px);
    border: 2px solid rgba(255, 255, 255, 0.2);
  }

  .image-expand-btn:hover {
    background: rgba(0, 90, 145, 0.95);
    transform: scale(1.15);
    box-shadow: 0 12px 35px rgba(0, 113, 185, 0.6);
  }

  .image-expand-btn svg {
    width: 24px;
    height: 24px;
    fill: white;
  }

  .text-image-caption {
    text-align: center;
    margin-top: 16px;
    color: var(--text-light);
    font-style: italic;
    font-weight: 500;
    padding: 8px 16px;
    background: rgba(0, 113, 185, 0.05);
    border-radius: 20px;
    display: inline-block;
  }

  /* Responsive Design */
