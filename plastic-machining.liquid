<!-- Dynamic CSS for responsive controls -->
<style>
  :root {
    --primary-color: #0071b9;
    --secondary-color: #005a91;
    --accent-color: #00a8e6;
    --text-dark: #1a1a1a;
    --text-light: #666;
    --white: #ffffff;
    --light-gray: #f8f9fa;
    --border-color: #e1e5e9;
    --shadow-light: 0 4px 20px rgba(0, 113, 185, 0.08);
    --shadow-medium: 0 8px 40px rgba(0, 113, 185, 0.12);
    --shadow-heavy: 0 20px 60px rgba(0, 113, 185, 0.2);
    --transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    --border-radius: 16px;
    --container-padding: clamp(20px, 5vw, 80px);
  }

  .plastic-machining-section {
    font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
    line-height: 1.6;
    color: var(--text-dark);
    overflow-x: hidden;
  }

  .plastic-machining-section * {
    box-sizing: border-box;
  }

  /* Consistent Section Padding */
  .section-padding {
    padding-top: var(--section-padding-top-desktop, 120px);
    padding-bottom: var(--section-padding-bottom-desktop, 120px);
  }

  @media (max-width: 1024px) {
    .section-padding {
      padding-top: var(--section-padding-top-tablet, 80px);
      padding-bottom: var(--section-padding-bottom-tablet, 80px);
    }
  }

  @media (max-width: 768px) {
    .section-padding {
      padding-top: var(--section-padding-top-mobile, 60px);
      padding-bottom: var(--section-padding-bottom-mobile, 60px);
    }
  }

  /* Hero Section Styles */
  {% assign hero_block = section.blocks | where: "type", "hero_section" | first %}
  {% if hero_block %}
    .plastic-machining-hero {
      --section-padding-top-desktop: {{ hero_block.settings.hero_padding_top_desktop | default: 120 }}px;
      --section-padding-bottom-desktop: {{ hero_block.settings.hero_padding_bottom_desktop | default: 120 }}px;
      --section-padding-top-tablet: {{ hero_block.settings.hero_padding_top_tablet | default: 80 }}px;
      --section-padding-bottom-tablet: {{ hero_block.settings.hero_padding_bottom_tablet | default: 80 }}px;
      --section-padding-top-mobile: {{ hero_block.settings.hero_padding_top_mobile | default: 60 }}px;
      --section-padding-bottom-mobile: {{ hero_block.settings.hero_padding_bottom_mobile | default: 60 }}px;
    }

    .hero-title {
      font-size: {{ hero_block.settings.hero_title_size | default: 64 }}px;
      line-height: {{ hero_block.settings.hero_title_line_height | default: 1.1 }};
      font-weight: 800;
      text-transform: uppercase;
      letter-spacing: -0.02em;
      margin-bottom: 24px;
      color: {{ hero_block.settings.hero_title_color | default: '#ffffff' }};
      user-select: text !important;
      -webkit-user-select: text !important;
      -moz-user-select: text !important;
      -ms-user-select: text !important;
      pointer-events: auto !important;
      cursor: text;
    }

    .hero-subtitle {
      font-size: {{ hero_block.settings.hero_subtitle_size | default: 20 }}px;
      line-height: {{ hero_block.settings.hero_subtitle_line_height | default: 1.6 }};
      margin-bottom: 40px;
      color: {{ hero_block.settings.hero_subtitle_color | default: 'rgba(255, 255, 255, 0.9)' }};
      font-weight: 300;
      user-select: text !important;
      -webkit-user-select: text !important;
      -moz-user-select: text !important;
      -ms-user-select: text !important;
      pointer-events: auto !important;
      cursor: text;
    }

    @media (max-width: 768px) {
      .hero-title {
        font-size: {{ hero_block.settings.hero_title_size_mobile | default: 32 }}px;
        line-height: {{ hero_block.settings.hero_title_line_height_mobile | default: 1.2 }};
      }

      .hero-subtitle {
        font-size: {{ hero_block.settings.hero_subtitle_size_mobile | default: 16 }}px;
        line-height: {{ hero_block.settings.hero_subtitle_line_height_mobile | default: 1.5 }};
      }
    }
  {% endif %}

  .plastic-machining-hero {
    position: relative;
    min-height: 100vh;
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--white);
    overflow: hidden;
  }

  .plastic-machining-hero::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: inherit;
    z-index: 0;
  }

  .hero-bg-image {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    background-attachment: fixed;
  }

  .hero-video-bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    z-index: 0;
  }

  .walkthrough-bg-image {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    background-attachment: fixed;
  }

  .hero-content {
    position: relative;
    z-index: 10;
    max-width: 1400px;
    width: 100%;
    padding: 0 var(--container-padding);
    text-align: center;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    pointer-events: auto;
  }

  .hero-buttons {
    display: flex;
    gap: 20px;
    justify-content: center;
    flex-wrap: wrap;
    margin-top: 40px;
  }

  .hero-btn {
    display: inline-flex;
    align-items: center;
    gap: 12px;
    padding: 18px 36px;
    text-decoration: none;
    border-radius: var(--border-radius);
    font-weight: 600;
    font-size: 18px;
    transition: var(--transition);
    box-shadow: var(--shadow-medium);
    position: relative;
    overflow: hidden;
    pointer-events: auto;
    cursor: pointer;
    z-index: 12;
  }

  .hero-btn-primary {
    background: linear-gradient(135deg, var(--accent-color) 0%, #0096d4 100%);
    color: var(--white);
  }

  .hero-btn-secondary {
    background: rgba(255, 255, 255, 0.1);
    color: var(--white);
    border: 2px solid rgba(255, 255, 255, 0.3);
    backdrop-filter: blur(10px);
  }

  .hero-btn-tertiary {
    background: transparent;
    color: var(--white);
    border: 2px solid var(--white);
  }

  .hero-btn:hover {
    transform: translateY(-2px);
    box-shadow: var(--shadow-heavy);
  }

  /* Consistent Section Title Styling */
  .tv-section-title {
    font-weight: 800;
    color: #0071b9;
    margin-bottom: 20px;
    text-transform: uppercase;
    letter-spacing: 4px;
    text-shadow: 0 2px 8px rgba(0, 113, 185, 0.3);
    position: relative;
    display: inline-block;
    line-height: 1.2;
    text-align: center;
    width: 100%;
  }

  .tv-section-title::before {
    content: '';
    position: absolute;
    bottom: -10px;
    left: 50%;
    transform: translateX(-50%);
    width: 100px;
    height: 4px;
    background: linear-gradient(90deg, transparent, #0071b9, transparent);
    border-radius: 2px;
  }

  .tv-section-subtitle {
    color: rgba(0, 113, 185, 0.8);
    line-height: 1.6;
    max-width: 700px;
    margin: 0 auto;
    font-weight: 300;
    text-align: center;
  }

  /* Services Overview Section */
  {% assign services_overview_block = section.blocks | where: "type", "services_overview" | first %}
  {% if services_overview_block %}
    .services-overview-section {
      --section-padding-top-desktop: {{ services_overview_block.settings.services_padding_top_desktop | default: 100 }}px;
      --section-padding-bottom-desktop: {{ services_overview_block.settings.services_padding_bottom_desktop | default: 100 }}px;
      --section-padding-top-tablet: {{ services_overview_block.settings.services_padding_top_tablet | default: 70 }}px;
      --section-padding-bottom-tablet: {{ services_overview_block.settings.services_padding_bottom_tablet | default: 70 }}px;
      --section-padding-top-mobile: {{ services_overview_block.settings.services_padding_top_mobile | default: 50 }}px;
      --section-padding-bottom-mobile: {{ services_overview_block.settings.services_padding_bottom_mobile | default: 50 }}px;
      background-color: {{ services_overview_block.settings.services_bg_color | default: '#ffffff' }};
    }

    .services-overview-title {
      font-size: {{ services_overview_block.settings.services_title_size | default: 42 }}px;
      color: {{ services_overview_block.settings.services_title_color | default: '#0071b9' }};
      font-weight: 700;
      text-align: center;
      margin-bottom: 30px;
      line-height: 1.2;
    }

    .services-overview-description {
      font-size: {{ services_overview_block.settings.services_description_size | default: 16 }}px;
      color: {{ services_overview_block.settings.services_description_color | default: '#333333' }};
      text-align: center;
      max-width: 800px;
      margin: 0 auto 60px;
      line-height: 1.6;
      font-weight: 400;
    }

    @media (max-width: 768px) {
      .services-overview-title {
        font-size: {{ services_overview_block.settings.services_title_size_mobile | default: 28 }}px;
        margin-bottom: 20px;
      }

      .services-overview-description {
        font-size: {{ services_overview_block.settings.services_description_size_mobile | default: 14 }}px;
        margin-bottom: 40px;
      }
    }
  {% endif %}

  .services-cards-grid {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: 30px;
    max-width: 1200px;
    margin: 0 auto;
  }

  .service-card {
    background: var(--white);
    border: 2px solid #e8f4f8;
    border-radius: 16px;
    padding: 40px 20px;
    text-align: center;
    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
    position: relative;
    overflow: hidden;
    min-height: 200px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }

  .service-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    transform: scaleX(0);
    transition: transform 0.3s ease;
  }

  .service-card:hover {
    transform: translateY(-8px);
    box-shadow: 0 20px 60px rgba(0, 113, 185, 0.15);
    border-color: var(--primary-color);
  }

  .service-card:hover::before {
    transform: scaleX(1);
  }

  .service-card-icon {
    width: 60px;
    height: 60px;
    background: linear-gradient(135deg, var(--primary-color), var(--accent-color));
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 auto 20px;
    transition: var(--transition);
  }

  .service-card-icon svg {
    width: 30px;
    height: 30px;
    color: var(--white);
  }

  .service-card-icon img {
    width: 30px;
    height: 30px;
    filter: brightness(0) invert(1);
  }

  .service-card-icon-image {
    background: transparent !important;
    padding: 0 !important;
    width: 70px !important;
    height: 70px !important;
  }

  .service-card-icon-image img {
    width: 100% !important;
    height: 100% !important;
    object-fit: contain !important;
    filter: none !important;
    border-radius: 8px;
  }

  .service-card:hover .service-card-icon {
    transform: scale(1.1) rotate(5deg);
  }

  .service-card-number {
    font-size: 28px;
    font-weight: 800;
    line-height: 1;
    margin-bottom: 8px;
    color: var(--primary-color);
  }

  .service-card-title {
    font-size: 16px;
    font-weight: 600;
    line-height: 1.3;
    margin: 0;
    color: var(--text-dark);
  }

  /* Responsive Design for Service Cards */
  @media (max-width: 1200px) {
    .services-cards-grid {
      grid-template-columns: repeat(2, 1fr);
      gap: 25px;
      max-width: 800px;
    }
  }

  @media (max-width: 768px) {
    .services-cards-grid {
      grid-template-columns: repeat(2, 1fr);
      gap: 20px;
      max-width: 100%;
    }

    .service-card {
      padding: 30px 15px;
      min-height: 180px;
    }

    .service-card-icon {
      width: 50px;
      height: 50px;
      margin-bottom: 15px;
    }

    .service-card-icon svg {
      width: 24px;
      height: 24px;
    }

    .service-card-number {
      font-size: 24px;
    }

    .service-card-title {
      font-size: 14px;
    }
  }

  @media (max-width: 480px) {
    .services-cards-grid {
      grid-template-columns: 1fr;
      gap: 15px;
    }

    .service-card {
      padding: 25px 15px;
      min-height: 160px;
    }

