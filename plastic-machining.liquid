    /* Enhanced Bullet Points with Dot Styling */
    .content-block-content ul {
      list-style: none;
      padding-left: 0;
      margin: 20px 0;
    }

    .content-block-content ul li {
      position: relative;
      padding-left: 40px;
      margin-bottom: 16px;
      font-size: inherit;
      line-height: inherit;
    }

    .content-block-content ul li::before {
      content: '';
      position: absolute;
      left: 10px;
      top: 50%;
      transform: translateY(-50%);
      width: 12px;
      height: 12px;
      background: var(--primary-color);
      border-radius: 50%;
      box-shadow: 0 0 0 4px rgba(0, 113, 185, 0.2);
      transition: all 0.3s ease;
    }

    .content-block-column:hover .content-block-content ul li::before {
      background: var(--accent-color);
      transform: translateY(-50%) scale(1.2);
    }

    .content-block-icon {
      width: 60px;
      height: 60px;
      margin-bottom: 20px;
      display: flex;
      align-items: center;
      justify-content: center;
    }

    .content-block-icon svg {
      width: 100%;
      height: 100%;
      fill: currentColor;
    }

    .content-block-icon img {
      width: 100%;
      height: 100%;
      object-fit: contain;
    }

    .content-block-image {
      margin-bottom: 20px;
      border-radius: var(--border-radius);
      overflow: hidden;
    }

    .content-block-image img {
      width: 100%;
      height: auto;
      object-fit: cover;
    }

    @media (max-width: 1024px) {
      .content-block-grid {
        gap: 40px;
      }

      .content-block-column {
        padding: 30px;
      }
    }

    @media (max-width: 768px) {
      .content-block-grid {
        grid-template-columns: 1fr;
        gap: 30px;
        padding: 0 15px;
      }

      .content-block-header {
        gap: 15px;
      }

      .content-block-plus-icon {
        width: 40px;
        height: 40px;
      }

      .content-block-plus-icon svg {
        width: 20px;
        height: 20px;
      }

      .content-block-column {
        padding: 25px;
      }

      .content-block-content {
        padding-left: 15px;
      }

      .content-block-content ul li {
        padding-left: 35px;
      }

      .content-block-content ul li::before {
        width: 10px;
        height: 10px;
        left: 8px;
        box-shadow: 0 0 0 3px rgba(0, 113, 185, 0.2);
      }
    }
  {% endif %}

  /* Print Styles */
  @media print {
    .plastic-machining-hero {
      background: var(--primary-color) !important;
      color: var(--white) !important;
    }

    .hero-video-bg,
    .hero-overlay {
      display: none !important;
    }
  }
</style>

<!-- Render blocks in the order they appear in the theme customizer -->
{% for block in section.blocks %}
