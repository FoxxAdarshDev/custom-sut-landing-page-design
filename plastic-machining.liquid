            {% if block.settings.hero_tertiary_cta_url != blank %}
              <a href="{{ block.settings.hero_tertiary_cta_url }}" class="hero-btn hero-btn-tertiary">
                {{ block.settings.hero_tertiary_cta_text | default: 'Contact Us' }}
              </a>
            {% endif %}
          </div>
        </div>

        {% if block.settings.background_type == 'image' and block.settings.hero_background_image != blank %}
          </div>
        {% endif %}
      </section>

    {% when 'text_image_block' %}
      <section class="text-image-block section-padding" style="
        --section-padding-top-desktop: {{ block.settings.text_image_padding_top_desktop | default: 80 }}px;
        --section-padding-bottom-desktop: {{ block.settings.text_image_padding_bottom_desktop | default: 80 }}px;
        --section-padding-top-tablet: {{ block.settings.text_image_padding_top_tablet | default: 60 }}px;
