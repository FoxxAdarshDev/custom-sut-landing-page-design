<div class="plastic-machining-main-container">
  <!-- Dynamic Blocks Content -->
  {% for block in section.blocks %}
    {% case block.type %}

      {% when 'hero_section' %}
        {% if block.settings.enable_hero_section %}
        <section class="plastic-machining-hero-section" {{ block.shopify_attributes }}>
          {% if block.settings.hero_content_display == 'image_only' %}
            {% if block.settings.hero_image %}
              <div class="plastic-machining-hero-image-container" style="padding-top: {{ block.settings.hero_padding_top | default: 0 }}px; padding-bottom: {{ block.settings.hero_padding_bottom | default: 0 }}px;">
                <img src="{{ block.settings.hero_image | img_url: '1920x' }}" alt="{{ block.settings.hero_title }}" class="plastic-machining-hero-image">
              </div>
            {% endif %}
          {% else %}
            {% if block.settings.hero_image %}
              <div class="plastic-machining-hero-background" style="background-image: url('{{ block.settings.hero_image | img_url: '1920x' }}');"></div>
              <div class="plastic-machining-hero-overlay" style="background-color: {{ block.settings.hero_overlay_color | default: '#003366' }}; opacity: {{ block.settings.hero_overlay_opacity | default: 80 | divided_by: 100.0 }}; padding-top: {{ block.settings.hero_padding_top | default: 80 }}px; padding-bottom: {{ block.settings.hero_padding_bottom | default: 80 }}px;">
                <div class="plastic-machining-container">
                  <div class="plastic-machining-hero-content">
                    <h1 class="plastic-machining-hero-title" style="font-size: {{ block.settings.hero_title_size_desktop | default: 56 }}px; color: {{ block.settings.hero_title_color | default: '#ffffff' }};">
                      {{ block.settings.hero_title }}
                    </h1>
                    <div class="plastic-machining-hero-description" style="font-size: {{ block.settings.hero_description_size_desktop | default: 20 }}px; color: {{ block.settings.hero_description_color | default: '#ffffff' }};">
                      {{ block.settings.hero_description }}
                    </div>
                    <div class="plastic-machining-hero-buttons">
                      {% if block.settings.hero_button1_text and block.settings.hero_button1_url %}
                        <a href="{{ block.settings.hero_button1_url }}" class="plastic-machining-hero-button" style="background: #00a86b; color: white;">
                          {{ block.settings.hero_button1_text }}
                        </a>
                      {% endif %}
                      {% if block.settings.hero_button2_text and block.settings.hero_button2_url %}
                        <a href="{{ block.settings.hero_button2_url }}" class="plastic-machining-hero-button" style="background: transparent; color: white; border: 2px solid white;">
                          {{ block.settings.hero_button2_text }}
                        </a>
                      {% endif %}
                      {% if block.settings.hero_button3_text and block.settings.hero_button3_url %}
                        <a href="{{ block.settings.hero_button3_url }}" class="plastic-machining-hero-button" style="background: #0071b9; color: white;">
                          {{ block.settings.hero_button3_text }}
                        </a>
                      {% endif %}
                    </div>
                  </div>
                </div>
              </div>
            {% else %}
              <div class="plastic-machining-hero-background" style="background: {{ block.settings.hero_background_color | default: '#0071b9' }}; padding-top: {{ block.settings.hero_padding_top | default: 80 }}px; padding-bottom: {{ block.settings.hero_padding_bottom | default: 80 }}px;">
                <div class="plastic-machining-container">
                  <div class="plastic-machining-hero-content">
                    <h1 class="plastic-machining-hero-title" style="font-size: {{ block.settings.hero_title_size_desktop | default: 56 }}px; color: {{ block.settings.hero_title_color | default: '#ffffff' }};">
                      {{ block.settings.hero_title }}
                    </h1>
                    <div class="plastic-machining-hero-description" style="font-size: {{ block.settings.hero_description_size_desktop | default: 20 }}px; color: {{ block.settings.hero_description_color | default: '#ffffff' }};">
                      {{ block.settings.hero_description }}
                    </div>
                    <div class="plastic-machining-hero-buttons">
                      {% if block.settings.hero_button1_text and block.settings.hero_button1_url %}
                        <a href="{{ block.settings.hero_button1_url }}" class="plastic-machining-hero-button" style="background: #00a86b; color: white;">
                          {{ block.settings.hero_button1_text }}
                        </a>
                      {% endif %}
                      {% if block.settings.hero_button2_text and block.settings.hero_button2_url %}
                        <a href="{{ block.settings.hero_button2_url }}" class="plastic-machining-hero-button" style="background: transparent; color: white; border: 2px solid white;">
                          {{ block.settings.hero_button2_text }}
                        </a>
                      {% endif %}
                      {% if block.settings.hero_button3_text and block.settings.hero_button3_url %}
                        <a href="{{ block.settings.hero_button3_url }}" class="plastic-machining-hero-button" style="background: #0071b9; color: white;">
                          {{ block.settings.hero_button3_text }}
                        </a>
                      {% endif %}
                    </div>
                  </div>
                </div>
              </div>
            {% endif %}
          {% endif %}
        </section>

        <style>
          @media (max-width: 768px) {
            .plastic-machining-hero-title {
              font-size: {{ block.settings.hero_title_size_mobile | default: 32 }}px !important;
            }
            .plastic-machining-hero-description {
              font-size: {{ block.settings.hero_description_size_mobile | default: 16 }}px !important;
            }
          }
        </style>
        {% endif %}

      {% when 'capabilities' %}
        <section class="plastic-machining-capabilities-section" {{ block.shopify_attributes }} style="padding-top: {{ block.settings.capabilities_padding_top | default: 100 }}px; padding-bottom: {{ block.settings.capabilities_padding_bottom | default: 100 }}px;">
          <div class="plastic-machining-container">
            <div class="plastic-machining-capabilities-header">
              <h2 class="plastic-machining-capabilities-title">{{ block.settings.capabilities_title }}</h2>
              <div class="plastic-machining-capabilities-description">{{ block.settings.capabilities_description }}</div>
            </div>
            <div class="plastic-machining-capabilities-grid">
              {% for capability_block in section.blocks %}
                {% if capability_block.type == 'capability_item' %}
                  <div class="plastic-machining-capability-card" {{ capability_block.shopify_attributes }}>
                    <div class="plastic-machining-capability-icon">{{ capability_block.settings.capability_icon }}</div>
                    <h3 class="plastic-machining-capability-name">{{ capability_block.settings.capability_name }}</h3>
                    <p class="plastic-machining-capability-desc">{{ capability_block.settings.capability_description }}</p>
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          </div>
        </section>

      {% when 'industries' %}
        <section class="plastic-machining-industries-section" {{ block.shopify_attributes }} style="padding-top: {{ block.settings.industries_padding_top | default: 100 }}px; padding-bottom: {{ block.settings.industries_padding_bottom | default: 100 }}px;">
          <div class="plastic-machining-container">
            <div class="plastic-machining-industries-header">
              <h2 class="plastic-machining-industries-title">{{ block.settings.industries_title }}</h2>
              <div class="plastic-machining-industries-description">{{ block.settings.industries_description }}</div>
            </div>
            <div class="plastic-machining-industries-slider">
              {% for industry_block in section.blocks %}
                {% if industry_block.type == 'industry_item' %}
                  <div class="plastic-machining-industry-item" {{ industry_block.shopify_attributes }}>
                    <div class="plastic-machining-industry-icon">{{ industry_block.settings.industry_icon }}</div>
                    <div class="plastic-machining-industry-name">{{ industry_block.settings.industry_name }}</div>
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          </div>
        </section>

      {% when 'why_choose' %}
        <section class="plastic-machining-why-choose-section" {{ block.shopify_attributes }} style="padding-top: {{ block.settings.why_choose_padding_top | default: 100 }}px; padding-bottom: {{ block.settings.why_choose_padding_bottom | default: 100 }}px;">
          <div class="plastic-machining-container">
            <div class="plastic-machining-why-choose-header">
              <h2 class="plastic-machining-why-choose-title">{{ block.settings.why_choose_title }}</h2>
              <div class="plastic-machining-why-choose-description">{{ block.settings.why_choose_description }}</div>
            </div>
            <div class="plastic-machining-why-choose-grid">
              {% for why_choose_item in section.blocks %}
                {% if why_choose_item.type == 'why_choose_item' %}
                  <div class="plastic-machining-why-choose-item" {{ why_choose_item.shopify_attributes }}>
                    <div class="plastic-machining-why-choose-icon">{{ why_choose_item.settings.why_choose_icon }}</div>
                    <div class="plastic-machining-why-choose-content">
                      <h3>{{ why_choose_item.settings.why_choose_heading }}</h3>
                      <p>{{ why_choose_item.settings.why_choose_text }}</p>
                    </div>
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          </div>
        </section>

      {% when 'showcase' %}
        <section class="plastic-machining-showcase-section" {{ block.shopify_attributes }} style="padding-top: {{ block.settings.showcase_padding_top | default: 100 }}px; padding-bottom: {{ block.settings.showcase_padding_bottom | default: 100 }}px;">
          <div class="plastic-machining-container">
            <div class="plastic-machining-showcase-header">
              <h2 class="plastic-machining-showcase-title">{{ block.settings.showcase_title }}</h2>
              <div class="plastic-machining-showcase-description">{{ block.settings.showcase_description }}</div>
            </div>
            <div class="plastic-machining-showcase-grid">
              {% for showcase_item in section.blocks %}
                {% if showcase_item.type == 'showcase_item' %}
                  <div class="plastic-machining-showcase-item" {{ showcase_item.shopify_attributes }}>
                    {% if showcase_item.settings.showcase_image %}
                      <img src="{{ showcase_item.settings.showcase_image | img_url: '400x' }}" alt="{{ showcase_item.settings.showcase_name }}" class="plastic-machining-showcase-image">
                    {% endif %}
                    <div class="plastic-machining-showcase-content">
                      <h3 class="plastic-machining-showcase-name">{{ showcase_item.settings.showcase_name }}</h3>
                      <p class="plastic-machining-showcase-desc">{{ showcase_item.settings.showcase_description }}</p>
                    </div>
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          </div>
        </section>

      {% when 'testimonials' %}
        <section class="plastic-machining-testimonials-section" {{ block.shopify_attributes }} style="padding-top: {{ block.settings.testimonials_padding_top | default: 100 }}px; padding-bottom: {{ block.settings.testimonials_padding_bottom | default: 100 }}px;">
          <div class="plastic-machining-container">
            <div class="plastic-machining-testimonials-header">
              <h2 class="plastic-machining-testimonials-title">{{ block.settings.testimonials_title }}</h2>
            </div>
            <div class="plastic-machining-testimonial-card">
              <div class="plastic-machining-testimonial-quote">{{ block.settings.testimonial_quote }}</div>
              <div class="plastic-machining-testimonial-author">{{ block.settings.testimonial_author }}</div>
            </div>
            <div class="plastic-machining-client-logos">
              {% for client_logo in section.blocks %}
                {% if client_logo.type == 'client_logo' and client_logo.settings.client_logo_image %}
                  <img src="{{ client_logo.settings.client_logo_image | img_url: '120x' }}" alt="{{ client_logo.settings.client_logo_alt }}" class="plastic-machining-client-logo" {{ client_logo.shopify_attributes }}>
                {% endif %}
              {% endfor %}
            </div>
          </div>
        </section>

      {% when 'quote_request' %}
        <section class="plastic-machining-quote-section" {{ block.shopify_attributes }} style="padding-top: {{ block.settings.quote_padding_top | default: 100 }}px; padding-bottom: {{ block.settings.quote_padding_bottom | default: 100 }}px;">
          <div class="plastic-machining-container">
            <div class="plastic-machining-quote-container">
              <div class="plastic-machining-quote-content">
                <h2>{{ block.settings.quote_title }}</h2>
                <div>{{ block.settings.quote_description }}</div>
              </div>
              <div class="plastic-machining-quote-form">
                <form action="{{ block.settings.quote_form_action }}" method="POST">
                  <div class="plastic-machining-form-group">
                    <label for="name">Name *</label>
                    <input type="text" id="name" name="name" required>
                  </div>
                  <div class="plastic-machining-form-group">
                    <label for="email">Email *</label>
                    <input type="email" id="email" name="email" required>
                  </div>
                  <div class="plastic-machining-form-group">
                    <label for="company">Company</label>
                    <input type="text" id="company" name="company">
                  </div>
                  <div class="plastic-machining-form-group">
                    <label for="message">Project Description *</label>
                    <textarea id="message" name="message" required></textarea>
                  </div>
                  <button type="submit" class="plastic-machining-btn plastic-machining-btn-primary">Send Quote Request</button>
                </form>
              </div>
            </div>
          </div>
        </section>

      {% when 'certifications' %}
        <section class="plastic-machining-certifications-section" {{ block.shopify_attributes }} style="padding-top: {{ block.settings.certifications_padding_top | default: 80 }}px; padding-bottom: {{ block.settings.certifications_padding_bottom | default: 80 }}px;">
          <div class="plastic-machining-container">
            <h2 class="plastic-machining-certifications-title">{{ block.settings.certifications_title }}</h2>
            <div class="plastic-machining-certifications-grid">
              {% for certification in section.blocks %}
                {% if certification.type == 'certification_item' %}
                  <div class="plastic-machining-certification-item" {{ certification.shopify_attributes }}>
                    <div class="plastic-machining-certification-icon">{{ certification.settings.certification_icon }}</div>
                    <div class="plastic-machining-certification-name">{{ certification.settings.certification_name }}</div>
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          </div>
        </section>

      {% when 'video_documentation' %}
        <section class="plastic-machining-video-section" {{ block.shopify_attributes }} style="padding-top: {{ block.settings.video_padding_top | default: 100 }}px; padding-bottom: {{ block.settings.video_padding_bottom | default: 100 }}px;">
          <div class="plastic-machining-container">
            <div class="plastic-machining-video-container">
              <div class="plastic-machining-video-content">
                <h2>{{ block.settings.video_title }}</h2>
                <div>{{ block.settings.video_description }}</div>
                <div class="plastic-machining-downloads">
                  {% if block.settings.brochure_url %}
                    <a href="{{ block.settings.brochure_url }}" class="plastic-machining-download-btn" target="_blank">
                      üìÑ Download Brochure
                    </a>
                  {% endif %}
                  {% if block.settings.catalog_url %}
                    <a href="{{ block.settings.catalog_url }}" class="plastic-machining-download-btn" target="_blank">
                      üìã Download Catalog
                    </a>
                  {% endif %}
                </div>
              </div>
              {% if block.settings.video_url %}
                <div class="plastic-machining-video-wrapper">
                  <iframe src="{{ block.settings.video_url }}" class="plastic-machining-video-embed" frameborder="0" allowfullscreen></iframe>
                </div>
              {% endif %}
            </div>
          </div>
        </section>

    {% endcase %}
  {% endfor %}
</div>