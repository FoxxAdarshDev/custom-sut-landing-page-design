              <div class="text-content-section">
                {% if block.settings.text_heading != blank %}
                  <h2 class="text-image-heading" style="
                    font-size: {{ block.settings.text_heading_size_desktop | default: 32 }}px;
                    line-height: {{ block.settings.text_heading_line_height | default: 1.3 }};
                    color: {{ block.settings.text_heading_color | default: '#0071b9' }};
                  ">{{ block.settings.text_heading }}</h2>
                {% endif %}

                {% if block.settings.text_description != blank %}
                  <div class="text-image-description" style="
                    font-size: {{ block.settings.text_description_size_desktop | default: 16 }}px;
                    line-height: {{ block.settings.text_description_line_height | default: 1.6 }};
                    color: {{ block.settings.text_description_color | default: '#333333' }};
                    --bullet-color: {{ block.settings.bullet_color | default: '#0071b9' }};
                    --bullet-size: {{ block.settings.bullet_size | default: 8 }}px;
                  ">
                    {{ block.settings.text_description }}
                  </div>
                {% endif %}
              </div>

              <!-- Image Content -->
              {% if block.settings.block_image != blank %}
                <div class="image-content-section">
                  <div class="text-image-visual-wrapper" style="border-radius: {{ block.settings.image_border_radius | default: 16 }}px;">
                    <img
                      src="{{ block.settings.block_image | img_url: '800x600' }}"
                      alt="{{ block.settings.image_caption | default: block.settings.text_heading }}"
                      class="text-image-img"
                      loading="lazy"
                      onclick="openImagePopup('{{ block.settings.block_image | img_url: '1200x900' }}')"
                    >

                    <button class="image-expand-btn" onclick="openImagePopup('{{ block.settings.block_image | img_url: '1200x900' }}')">
                      <svg viewBox="0 0 24 24">
                        <path d="M19,19H5V5H12V3H5C3.89,3 3,3.89 3,5V19A2,2 0 0,0 5,21H19A2,2 0 0,0 21,19V12H19M14,3V5H17.59L7.76,14.83L9.17,16.24L19,6.41V10H21V3"/>
                      </svg>
                    </button>
                  </div>

                  {% if block.settings.image_caption != blank %}
                    <p class="text-image-caption" style="
                      font-size: {{ block.settings.image_caption_size_desktop | default: 14 }}px;
                      color: {{ block.settings.image_caption_color | default: '#666666' }};
                    ">{{ block.settings.image_caption }}</p>
                  {% endif %}
                </div>
              {% endif %}
            </div>
          </div>
        </div>

        <!-- Mobile-specific styles -->
        <style>
          @media (max-width: 768px) {
            .text-image-heading {
              font-size: {{ block.settings.text_heading_size_mobile | default: 24 }}px !important;
            }
            .text-image-description {
              font-size: {{ block.settings.text_description_size_mobile | default: 14 }}px !important;
            }
            .text-image-caption {
              font-size: {{ block.settings.image_caption_size_mobile | default: 12 }}px !important;
            }
          }
        </style>
      </section>

    {% when 'capabilities_section' %}
