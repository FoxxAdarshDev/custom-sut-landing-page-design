<style>
  .plastic-machining-main-container {
    max-width: 100%;
    width: 100%;
    margin: 0 auto;
    overflow-x: hidden;
    box-sizing: border-box;
  }

  /* Global Styles */
  .plastic-machining-container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .plastic-machining-btn {
    display: inline-block;
    padding: 12px 30px;
    text-decoration: none;
    font-weight: 600;
    font-size: 14px;
    letter-spacing: 1px;
    text-transform: uppercase;
    border-radius: 5px;
    transition: all 0.3s ease;
    border: none;
    cursor: pointer;
    text-align: center;
  }

  .plastic-machining-btn-primary {
    background: #0071b9;
    color: white;
    border: 2px solid #0071b9;
  }

  .plastic-machining-btn-primary:hover {
    background: rgba(0, 113, 185, 0.8);
    transform: translateY(-2px);
  }

  .plastic-machining-btn-secondary {
    background: transparent;
    color: #0071b9;
    border: 2px solid #0071b9;
  }

  .plastic-machining-btn-secondary:hover {
    background: #0071b9;
    color: white;
  }

  /* Hero Section */
  .plastic-machining-hero-section {
    position: relative;
    min-height: 60vh;
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
  }

  .plastic-machining-hero-image-container {
    text-align: center;
    width: 100%;
  }

  .plastic-machining-hero-image {
    max-width: 100%;
    height: auto;
    display: block;
    width: 100%;
  }

  .plastic-machining-hero-background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    z-index: 1;
  }

  .plastic-machining-hero-overlay {
    position: relative;
    z-index: 3;
    width: 100%;
    min-height: 60vh;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .plastic-machining-hero-content {
    text-align: center;
    color: white;
    max-width: 900px;
    width: 100%;
    padding: 0 20px;
  }

  .plastic-machining-hero-title {
    font-weight: 700;
    line-height: 1.1;
    margin-bottom: 25px;
    text-transform: uppercase;
    letter-spacing: 2px;
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.5);
  }

  .plastic-machining-hero-description {
    line-height: 1.6;
    margin-bottom: 30px;
    text-shadow: 0 1px 3px rgba(0, 0, 0, 0.5);
  }

  .plastic-machining-hero-buttons {
    display: flex;
    gap: 20px;
    justify-content: center;
    flex-wrap: wrap;
  }

  .plastic-machining-hero-button {
    display: inline-block;
    padding: 18px 40px;
    text-decoration: none;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1px;
    border: none;
    border-radius: 8px;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  /* Capabilities Section */
  .plastic-machining-capabilities-section {
    background: #f8f9fa;
  }

  .plastic-machining-capabilities-header {
    text-align: center;
    margin-bottom: 60px;
  }

  .plastic-machining-capabilities-title {
    font-size: 48px;
    color: #333;
    font-weight: 700;
    margin-bottom: 20px;
  }

  .plastic-machining-capabilities-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 30px;
  }

  .plastic-machining-capability-card {
    background: white;
    padding: 40px 30px;
    border-radius: 15px;
    text-align: center;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease;
  }

  .plastic-machining-capability-card:hover {
    transform: translateY(-5px);
  }

  .plastic-machining-capability-icon {
    font-size: 48px;
    color: #0071b9;
    margin-bottom: 20px;
  }

  .plastic-machining-capability-name {
    font-size: 24px;
    font-weight: 700;
    color: #333;
    margin-bottom: 15px;
  }

  .plastic-machining-capability-desc {
    font-size: 16px;
    color: #666;
    line-height: 1.5;
  }

  /* Industries Section */
  .plastic-machining-industries-section {
    background: white;
  }

  .plastic-machining-industries-header {
    text-align: center;
    margin-bottom: 60px;
  }

  .plastic-machining-industries-title {
    font-size: 48px;
    color: #333;
    font-weight: 700;
    margin-bottom: 20px;
  }

  .plastic-machining-industries-slider {
    display: flex;
    justify-content: center;
    gap: 60px;
    align-items: center;
    flex-wrap: wrap;
  }

  .plastic-machining-industry-item {
    text-align: center;
    flex: 1;
    min-width: 200px;
  }

  .plastic-machining-industry-icon {
    font-size: 64px;
    color: #0071b9;
    margin-bottom: 20px;
  }

  .plastic-machining-industry-name {
    font-size: 20px;
    font-weight: 600;
    color: #333;
  }

  /* Why Choose Us Section */
  .plastic-machining-why-choose-section {
    background: linear-gradient(135deg, #0071b9 0%, #005a91 100%);
    color: white;
  }

  .plastic-machining-why-choose-header {
    text-align: center;
    margin-bottom: 60px;
  }

  .plastic-machining-why-choose-title {
    font-size: 48px;
    font-weight: 700;
    margin-bottom: 20px;
  }

  .plastic-machining-why-choose-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 40px;
  }

  .plastic-machining-why-choose-item {
    display: flex;
    align-items: flex-start;
    gap: 20px;
  }

  .plastic-machining-why-choose-icon {
    font-size: 32px;
    color: #00a86b;
    margin-top: 5px;
  }

  .plastic-machining-why-choose-content h3 {
    font-size: 20px;
    font-weight: 600;
    margin-bottom: 10px;
  }

  .plastic-machining-why-choose-content p {
    font-size: 16px;
    opacity: 0.9;
    line-height: 1.5;
  }

  /* Showcase Section */
  .plastic-machining-showcase-section {
    background: #f8f9fa;
  }

  .plastic-machining-showcase-header {
    text-align: center;
    margin-bottom: 60px;
  }

  .plastic-machining-showcase-title {
    font-size: 48px;
    color: #333;
    font-weight: 700;
    margin-bottom: 20px;
  }

  .plastic-machining-showcase-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    gap: 30px;
  }

  .plastic-machining-showcase-item {
    background: white;
    border-radius: 15px;
    overflow: hidden;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease;
  }

  .plastic-machining-showcase-item:hover {
    transform: translateY(-5px);
  }

  .plastic-machining-showcase-image {
    width: 100%;
    height: 220px;
    object-fit: cover;
  }

  .plastic-machining-showcase-content {
    padding: 25px;
  }

  .plastic-machining-showcase-name {
    font-size: 20px;
    font-weight: 700;
    color: #333;
    margin-bottom: 10px;
  }

  .plastic-machining-showcase-desc {
    font-size: 16px;
    color: #666;
    line-height: 1.5;
  }

  /* Testimonials Section */
  .plastic-machining-testimonials-section {
    background: white;
  }

  .plastic-machining-testimonials-header {
    text-align: center;
    margin-bottom: 60px;
  }

  .plastic-machining-testimonials-title {
    font-size: 48px;
    color: #333;
    font-weight: 700;
    margin-bottom: 20px;
  }

  .plastic-machining-testimonial-card {
    background: #f8f9fa;
    padding: 40px;
    border-radius: 15px;
    text-align: center;
    max-width: 800px;
    margin: 0 auto;
  }

  .plastic-machining-testimonial-quote {
    font-size: 24px;
    font-style: italic;
    color: #333;
    margin-bottom: 30px;
    line-height: 1.5;
  }

  .plastic-machining-testimonial-author {
    font-size: 18px;
    font-weight: 600;
    color: #0071b9;
  }

  .plastic-machining-client-logos {
    display: flex;
    justify-content: center;
    gap: 40px;
    margin-top: 40px;
    flex-wrap: wrap;
  }

  .plastic-machining-client-logo {
    height: 60px;
    width: auto;
    filter: grayscale(100%);
    transition: filter 0.3s ease;
  }

  .plastic-machining-client-logo:hover {
    filter: grayscale(0%);
  }

  /* Quote Section */
  .plastic-machining-quote-section {
    background: linear-gradient(135deg, #003366 0%, #0071b9 100%);
    color: white;
  }

  .plastic-machining-quote-container {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: center;
  }

  .plastic-machining-quote-content h2 {
    font-size: 42px;
    font-weight: 700;
    margin-bottom: 25px;
  }

  .plastic-machining-quote-content p {
    font-size: 18px;
    margin-bottom: 30px;
    opacity: 0.9;
  }

  .plastic-machining-quote-form {
    background: rgba(255, 255, 255, 0.1);
    padding: 40px;
    border-radius: 15px;
  }

  .plastic-machining-form-group {
    margin-bottom: 20px;
  }

  .plastic-machining-form-group label {
    display: block;
    margin-bottom: 8px;
    font-weight: 600;
  }

  .plastic-machining-form-group input,
  .plastic-machining-form-group textarea {
    width: 100%;
    padding: 12px;
    border: none;
    border-radius: 5px;
    font-size: 16px;
  }

  .plastic-machining-form-group textarea {
    height: 100px;
    resize: vertical;
  }

  /* Certifications Section */
  .plastic-machining-certifications-section {
    background: #f8f9fa;
    text-align: center;
  }

  .plastic-machining-certifications-title {
    font-size: 48px;
    color: #333;
    font-weight: 700;
    margin-bottom: 40px;
  }

  .plastic-machining-certifications-grid {
    display: flex;
    justify-content: center;
    gap: 60px;
    flex-wrap: wrap;
  }

  .plastic-machining-certification-item {
    text-align: center;
  }

  .plastic-machining-certification-icon {
    font-size: 48px;
    color: #0071b9;
    margin-bottom: 15px;
  }

  .plastic-machining-certification-name {
    font-size: 18px;
    font-weight: 600;
    color: #333;
  }

  /* Video Documentation Section */
  .plastic-machining-video-section {
    background: white;
  }

  .plastic-machining-video-container {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: center;
  }

  .plastic-machining-video-content h2 {
    font-size: 42px;
    color: #333;
    font-weight: 700;
    margin-bottom: 25px;
  }

  .plastic-machining-video-content p {
    font-size: 18px;
    color: #666;
    margin-bottom: 30px;
    line-height: 1.6;
  }

  .plastic-machining-video-wrapper {
    position: relative;
    border-radius: 15px;
    overflow: hidden;
    box-shadow: 0 15px 35px rgba(0, 0, 0, 0.1);
  }

  .plastic-machining-video-embed {
    width: 100%;
    min-height: 300px;
    border: none;
    border-radius: 15px;
  }

  .plastic-machining-downloads {
    display: flex;
    gap: 20px;
    flex-wrap: wrap;
  }

  .plastic-machining-download-btn {
    display: flex;
    align-items: center;
    gap: 10px;
    padding: 12px 25px;
    background: #0071b9;
    color: white;
    text-decoration: none;
    border-radius: 5px;
    font-weight: 600;
    transition: background 0.3s ease;
  }

  .plastic-machining-download-btn:hover {
    background: #005a91;
  }

  /* Mobile Responsive */
  @media (max-width: 768px) {
    .plastic-machining-hero-buttons {
      flex-direction: column;
      align-items: center;
    }

    .plastic-machining-hero-title {
      font-size: 28px;
    }

    .plastic-machining-hero-description {
      font-size: 16px;
    }

    .plastic-machining-capabilities-title,
    .plastic-machining-industries-title,
    .plastic-machining-why-choose-title,
    .plastic-machining-showcase-title,
    .plastic-machining-testimonials-title,
    .plastic-machining-certifications-title {
      font-size: 32px;
    }

    .plastic-machining-quote-container,
    .plastic-machining-video-container {
      grid-template-columns: 1fr;
      gap: 40px;
    }

    .plastic-machining-industries-slider {
      gap: 30px;
    }

    .plastic-machining-why-choose-grid {
      grid-template-columns: 1fr;
    }

    .plastic-machining-testimonial-quote {
      font-size: 20px;
    }

    .plastic-machining-client-logos {
      gap: 20px;
    }

    .plastic-machining-certifications-grid {
      gap: 30px;
    }
  }

  @media (max-width: 480px) {
    .plastic-machining-hero-title {
      font-size: 24px;
    }

    .plastic-machining-capabilities-title,
    .plastic-machining-industries-title,
    .plastic-machining-why-choose-title,
    .plastic-machining-showcase-title,
    .plastic-machining-testimonials-title,
    .plastic-machining-certifications-title {
      font-size: 28px;
    }

    .plastic-machining-quote-content h2,
    .plastic-machining-video-content h2 {
      font-size: 28px;
    }

    .plastic-machining-testimonial-quote {
      font-size: 18px;
    }
  }
