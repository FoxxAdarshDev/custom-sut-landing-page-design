        <script>
        // Labour Day Sale Countdown Timer for Navigation
        function updateNavCountdown() {
            const countDownDate = new Date('2025-09-30T23:59:59').getTime();
            const now = new Date().getTime();
            const distance = countDownDate - now;

            if (distance > 0) {
                const days = Math.floor(distance / (1000 * 60 * 60 * 24));
                const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));

                const navDaysEl = document.getElementById("nav-days");
                const navHoursEl = document.getElementById("nav-hours");
                const navMinsEl = document.getElementById("nav-mins");

                if (navDaysEl) navDaysEl.textContent = String(days).padStart(2, '0');
                if (navHoursEl) navHoursEl.textContent = String(hours).padStart(2, '0');
                if (navMinsEl) navMinsEl.textContent = String(minutes).padStart(2, '0');
            } else {
                // Reset timer if expired
                ["nav-days", "nav-hours", "nav-mins"].forEach(id => {
                    const element = document.getElementById(id);
                    if (element) element.textContent = "00";
                });
            }
        }

        // Start countdown for navigation
        document.addEventListener('DOMContentLoaded', function() {
            updateNavCountdown();
            setInterval(updateNavCountdown, 60000); // Update every minute
        });
        </script>

