        <style>
        .labour-day-sale-badge {
            position: absolute;
            top: -45px;
            left: 50%;
            transform: translateX(-50%);
            background: linear-gradient(135deg, #1e3a8a 0%, #dc2626 50%, #1e3a8a 100%);
            border-radius: 12px;
            padding: 8px 12px;
            box-shadow: 0 4px 15px rgba(220, 38, 38, 0.4);
            z-index: 1000;
            min-width: 180px;
            border: 2px solid #ffd700;
            animation: patrioticGlow 2s ease-in-out infinite;
        }

        @keyframes patrioticGlow {
            0%, 100% { box-shadow: 0 4px 15px rgba(220, 38, 38, 0.4); }
            50% { box-shadow: 0 6px 25px rgba(255, 215, 0, 0.6); }
        }

        .sale-banner {
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 8px;
            margin-bottom: 6px;
        }

        .flag-icon {
            font-size: 14px;
            animation: wave 1.5s ease-in-out infinite;
        }

        @keyframes wave {
            0%, 100% { transform: rotate(0deg); }
            50% { transform: rotate(10deg); }
        }

        .sale-text {
            color: #ffd700;
            font-weight: 900;
            font-size: 12px;
            text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5);
            letter-spacing: 1px;
        }

        .countdown-mini {
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 8px;
        }

        .countdown-label {
            color: white;
            font-size: 9px;
            font-weight: 700;
            text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.5);
        }

        .countdown-units {
            display: flex;
            gap: 4px;
        }

        .time-unit {
            background: rgba(255, 255, 255, 0.9);
            color: #1e3a8a;
            padding: 2px 4px;
            border-radius: 4px;
            font-size: 8px;
            font-weight: 700;
            min-width: 20px;
            text-align: center;
            border: 1px solid #ffd700;
        }

        .time-unit span {
            color: #dc2626;
            font-weight: 900;
        }

        /* Mobile adjustments */
        @media (max-width: 768px) {
            .labour-day-sale-badge {
                top: -40px;
                min-width: 160px;
                padding: 6px 10px;
            }
            
            .sale-text {
                font-size: 10px;
            }
            
            .countdown-label {
                font-size: 8px;
            }
            
            .time-unit {
                font-size: 7px;
                padding: 1px 3px;
                min-width: 18px;
            }
