   <script>
          window.addEventListener('load', function() {
              // Replace first slide image if it's not a video
              const firstSlide = document.querySelector('.slick-slide[data-slick-index="0"]');
              if(firstSlide) {
                  const firstSlideImg = firstSlide.querySelector('img');
                  const firstSlideVideo = firstSlide.querySelector('video');
                  
                  // Only replace image if there's no video
                  if(firstSlideImg && !firstSlideVideo) {
                      firstSlideImg.src = 'https://cdn.shopify.com/s/files/1/1064/0118/files/Rhino_carboy_and_bottles_promo-foxx-life-sciences-home-slideshow-v2.jpg?v=1750361081';
