                                function adjustColumnWidth() {
                                    // Select all elements with the class 'content-column'
                                    const contentColumns = document.querySelectorAll('.content-column');

                                    // Use devicePixelRatio to determine if the browser is zoomed
                                    const zoomLevel = window.devicePixelRatio;

                                    contentColumns.forEach(contentColumn => {
                                        if (zoomLevel >= 1 && zoomLevel < 1.1) {
                                            // If the zoom level is between 100% and 110%
                                            contentColumn.style.width = '421px'; // Set to default width
                                        } else if (zoomLevel >= 1.1) {
                                            // If the zoom level is 110% or more
                                            contentColumn.style.width = '100%'; // Set to full width
                                        } else {
                                            // If zoom is less than 100%
                                            contentColumn.style.width = '421px'; // Set to default width
                                        }
                                    });
                                }

                                // Adjust on load
                                window.onload = adjustColumnWidth;

                                // Adjust on resize
                                window.onresize = adjustColumnWidth;
