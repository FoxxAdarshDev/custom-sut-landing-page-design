              // Replace second slide image if it's not a video
              const secondSlide = document.querySelector('.slick-slide[data-slick-index="1"]');
              if(secondSlide) {
                  const secondSlideImg = secondSlide.querySelector('img');
                  const secondSlideVideo = secondSlide.querySelector('video');
                  
                  // Only replace image if there's no video
                  if(secondSlideImg && !secondSlideVideo) {
