  <div class="tab-switch__content" data-content="desc">
    <div class="product-description rte" itemprop="description">
      {{ product.description }}
      <script>
        document.addEventListener("DOMContentLoaded", function() {
          // Select the product description container
          const descriptionContainer = document.querySelector('.product-description');
          
          // If the description container exists
          if (descriptionContainer) {
            // Find all anchor tags within this container that start with "https://amzn.to"
            const links = descriptionContainer.querySelectorAll('a[href^="https://amzn.to"]');
            
            // Iterate through each link and check its parent element
            links.forEach(link => {
              // Hide the link by setting display to none
              link.style.display = 'none';
      
              // Find the closest heading element
              let headingElement = link.closest('h1, h2, h3, h4, h5, h6');
      
              // Hide the heading element if found
              if (headingElement) {
                headingElement.style.display = 'none';
              }
            });
          }
        });
      </script>
      
      
    </div>
  </div>
