                  var originalPriceElement = document.getElementById('productPrice');
                  var originalPrice = parseFloat(originalPriceElement.getAttribute('data-original-price'));
                  var discountPercentage = parseFloat(originalPriceElement.getAttribute('data-discount-percentage'));
              
                  // Calculate the full price before discount
                  var fullPrice = originalPrice / (1 - (discountPercentage / 100));
              
                  // Calculate the discount amount
                  var discountAmount = fullPrice - originalPrice;
              
                  // Update the ListedPrice span with the full price
                  var listedPriceElement = document.getElementById('ListedPrice');
                  listedPriceElement.innerHTML = '$' + fullPrice.toFixed(2) + ' USD';
              
                  // Update the DiscountAmount span with the savings
