{% schema %}
{
  "name": "Meet Our Leaders",
  "class": "team-section",
  "settings": [
    {
      "type": "header",
      "content": "Section Settings"
    },
    {
      "type": "text",
      "id": "section_title",
      "label": "Section Title",
      "default": "MEET OUR LEADERS"
    },
    {
      "type": "textarea",
      "id": "section_description",
      "label": "Section Description",
      "default": "Our leadership team brings decades of experience and innovative thinking to drive our mission forward."
    },
    {
      "type": "header",
      "content": "Filter Tabs"
    },
    {
      "type": "checkbox",
      "id": "enable_filters",
      "label": "Enable Filter Tabs",
      "default": true
    },
    {
      "type": "text",
      "id": "filter_all_text",
      "label": "All Filter Text",
      "default": "All"
    },
    {
      "type": "text",
      "id": "filter_executive_text",
      "label": "Executive Filter Text",
      "default": "Executive Leadership"
    },
    {
      "type": "text",
      "id": "filter_management_text",
      "label": "Management Filter Text",
      "default": "Management Team"
    },
    {
      "type": "text",
      "id": "filter_advisory_text",
      "label": "Advisory Filter Text",
      "default": "Advisory Board"
    }
  ],
  "blocks": [
    {
      "type": "team_member",
      "name": "Team Member",
      "settings": [
        {
          "type": "image_picker",
          "id": "member_image",
          "label": "Member Photo"
        },
        {
          "type": "text",
          "id": "member_name",
          "label": "Full Name",
          "default": "John Doe"
        },
        {
          "type": "text",
          "id": "member_position",
          "label": "Position/Title",
          "default": "Chief Executive Officer"
        },
        {
          "type": "textarea",
          "id": "member_bio",
          "label": "Bio/Description"
        },
        {
          "type": "select",
          "id": "member_category",
          "label": "Team Category",
          "default": "executive",
          "options": [
            {
              "value": "executive",
              "label": "Executive Leadership"
            },
            {
              "value": "management",
              "label": "Management Team"
            },
            {
              "value": "advisory",
              "label": "Advisory Board"
            }
          ]
        },
        {
          "type": "select",
          "id": "card_color",
          "label": "Card Background Color",
          "default": "blue",
          "options": [
            {
              "value": "blue",
              "label": "Blue"
            },
            {
              "value": "orange",
              "label": "Orange"
            },
            {
              "value": "purple",
              "label": "Purple"
            },
            {
              "value": "green",
              "label": "Green"
            },
            {
              "value": "red",
              "label": "Red"
            }
          ]
        },
        {
          "type": "header",
          "content": "Social Media Links"
        },
        {
          "type": "url",
          "id": "linkedin_url",
          "label": "LinkedIn URL"
        },
        {
          "type": "url",
          "id": "twitter_url",
          "label": "Twitter URL"
        },
        {
          "type": "url",
          "id": "email",
          "label": "Email Address"
        },
        {
          "type": "url",
          "id": "website_url",
          "label": "Personal Website"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Meet Our Leaders",
      "category": "Team",
      "blocks": [
        {
          "type": "team_member",
          "settings": {
            "member_name": "John Cooling",
            "member_position": "President & CEO",
            "member_category": "executive",
            "card_color": "blue"
          }
        },
        {
          "type": "team_member",
          "settings": {
            "member_name": "Kevin Cooling",
            "member_position": "VP of Manufacturing",
            "member_category": "executive",
            "card_color": "orange"
          }
        },
        {
          "type": "team_member",
          "settings": {
            "member_name": "Jeff Meade",
            "member_position": "Senior VP of Sales",
            "member_category": "executive",
            "card_color": "purple"
          }
        }
      ]
    }
  ]
}
{% endschema %}

<!-- Meet Our Leaders Section -->
<section class="team-section" data-section-id="{{ section.id }}">
  <div class="team-container">
    <!-- Section Header -->
    <div class="team-header">
      <h2 class="team-title">{{ section.settings.section_title | default: 'MEET OUR LEADERS' }}</h2>
      {% if section.settings.section_description != blank %}
        <p class="team-description">{{ section.settings.section_description }}</p>
      {% endif %}
    </div>

    <!-- Filter Tabs -->
    {% if section.settings.enable_filters %}
      <div class="team-filters">
        <button class="filter-btn active" data-filter="all">
          {{ section.settings.filter_all_text | default: 'All' }}
        </button>
        <button class="filter-btn" data-filter="executive">
          {{ section.settings.filter_executive_text | default: 'Executive Leadership' }}
        </button>
        <button class="filter-btn" data-filter="management">
          {{ section.settings.filter_management_text | default: 'Management Team' }}
        </button>
        <button class="filter-btn" data-filter="advisory">
          {{ section.settings.filter_advisory_text | default: 'Advisory Board' }}
        </button>
      </div>
    {% endif %}

    <!-- Team Grid -->
    <div class="team-grid">
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'team_member' %}
            <div class="team-card" data-category="{{ block.settings.member_category | default: 'executive' }}" data-aos="fade-up" data-aos-delay="{{ forloop.index0 | times: 100 }}">
              <div class="card-background card-{{ block.settings.card_color | default: 'blue' }}"></div>

              <div class="member-image-container">
                {% if block.settings.member_image != blank %}
                  <img src="{{ block.settings.member_image | img_url: '300x300' }}" 
                       alt="{{ block.settings.member_name }}" 
                       class="member-image"
                       loading="lazy">
                {% else %}
                  <div class="member-image-placeholder">
                    <svg viewBox="0 0 24 24" fill="currentColor">
                      <path d="M12 12c2.21 0 4-1.79 4-4s-1.79-4-4-4-4 1.79-4 4 1.79 4 4 4zm0 2c-2.67 0-8 1.34-8 4v2h16v-2c0-2.66-5.33-4-8-4z"/>
                    </svg>
                  </div>
                {% endif %}

                <!-- Social Media Links -->
                <div class="social-links">
                  {% if block.settings.linkedin_url != blank %}
                    <a href="{{ block.settings.linkedin_url }}" target="_blank" rel="noopener" class="social-link linkedin">
                      <svg viewBox="0 0 24 24" fill="currentColor">
                        <path d="M20.447 20.452h-3.554v-5.569c0-1.328-.027-3.037-1.852-3.037-1.853 0-2.136 1.445-2.136 2.939v5.667H9.351V9h3.414v1.561h.046c.477-.9 1.637-1.85 3.37-1.85 3.601 0 4.267 2.37 4.267 5.455v6.286zM5.337 7.433c-1.144 0-2.063-.926-2.063-2.065 0-1.138.92-2.063 2.063-2.063 1.14 0 2.064.925 2.064 2.063 0 1.139-.925 2.065-2.064 2.065zm1.782 13.019H3.555V9h3.564v11.452zM22.225 0H1.771C.792 0 0 .774 0 1.729v20.542C0 23.227.792 24 1.771 24h20.451C23.2 24 24 23.227 24 22.271V1.729C24 .774 23.2 0 22.222 0h.003z"/>
                      </svg>
                    </a>
                  {% endif %}

                  {% if block.settings.twitter_url != blank %}
                    <a href="{{ block.settings.twitter_url }}" target="_blank" rel="noopener" class="social-link twitter">
                      <svg viewBox="0 0 24 24" fill="currentColor">
                        <path d="M23.953 4.57a10 10 0 01-2.825.775 4.958 4.958 0 002.163-2.723c-.951.555-2.005.959-3.127 1.184a4.92 4.92 0 00-8.384 4.482C7.69 8.095 4.067 6.13 1.64 3.162a4.822 4.822 0 00-.666 2.475c0 1.71.87 3.213 2.188 4.096a4.904 4.904 0 01-2.228-.616v.06a4.923 4.923 0 003.946 4.827 4.996 4.996 0 01-2.212.085 4.936 4.936 0 004.604 3.417 9.867 9.867 0 01-6.102 2.105c-.39 0-.779-.023-1.17-.067a13.995 13.995 0 007.557 2.209c9.053 0 13.998-7.496 13.998-13.985 0-.21 0-.42-.015-.63A9.935 9.935 0 0024 4.59z"/>
                      </svg>
                    </a>
                  {% endif %}

                  {% if block.settings.email != blank %}
                    <a href="mailto:{{ block.settings.email }}" class="social-link email">
                      <svg viewBox="0 0 24 24" fill="currentColor">
                        <path d="M20 4H4c-1.1 0-1.99.9-1.99 2L2 18c0 1.1.9 2 2 2h16c1.1 0 2-.9 2-2V6c0-1.1-.9-2-2-2zm0 4l-8 5-8-5V6l8 5 8-5v2z"/>
                      </svg>
                    </a>
                  {% endif %}

                  {% if block.settings.website_url != blank %}
                    <a href="{{ block.settings.website_url }}" target="_blank" rel="noopener" class="social-link website">
                      <svg viewBox="0 0 24 24" fill="currentColor">
                        <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-1 17.93c-3.94-.49-7-3.85-7-7.93 0-.62.08-1.21.21-1.79L9 15v1c0 1.1.9 2 2 2v1.93zm6.9-2.54c-.26-.81-1-1.39-1.9-1.39h-1v-3c0-.55-.45-1-1-1H8v-2h2c.55 0 1-.45 1-1V7h2c1.1 0 2-.9 2-2v-.41c2.93 1.19 5 4.06 5 7.41 0 2.08-.8 3.97-2.1 5.39z"/>
                      </svg>
                    </a>
                  {% endif %}
                </div>

                <!-- Info Badge -->
                <div class="info-badge">
                  <svg viewBox="0 0 24 24" fill="currentColor">
                    <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-6h2v6zm0-8h-2V7h2v2z"/>
                  </svg>
                </div>
              </div>

              <div class="member-content">
                <h3 class="member-name">{{ block.settings.member_name | default: 'Team Member' }}</h3>
                <p class="member-position">{{ block.settings.member_position | default: 'Position' }}</p>

                {% if block.settings.member_bio != blank %}
                  <div class="member-bio">
                    <p>{{ block.settings.member_bio }}</p>
                  </div>
                {% endif %}
              </div>

              <!-- Hover Overlay -->

            </div>
        {% endcase %}
      {% endfor %}
    </div>

    <!-- Empty State -->
    <div class="empty-state" style="display: none;">
      <p>No team members found for the selected category.</p>
    </div>
  </div>

  <!-- Floating Elements -->
  <div class="floating-elements">
    <div class="floating-element element-1"></div>
    <div class="floating-element element-2"></div>
    <div class="floating-element element-3"></div>
    <div class="floating-element element-4"></div>
  </div>
</section>

<style>
/* Team Section */
.team-section {
  position: relative;
  padding: 120px 0;
  background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);
  overflow: hidden;
}

.team-container {
  max-width: 1400px;
  width: 100%;
  margin: 0 auto;
  padding: 0 20px;
  position: relative;
  z-index: 2;
}

/* Header */
.team-header {
  text-align: center;
  margin-bottom: 80px;
}

.team-title {
  font-size: 64px;
  font-weight: 800;
  color: #0071b9;
  margin-bottom: 20px;
  text-transform: uppercase;
  letter-spacing: 4px;
  position: relative;
  display: inline-block;
}

.team-title::before {
  content: '';
  position: absolute;
  bottom: -10px;
  left: 50%;
  transform: translateX(-50%);
  width: 100px;
  height: 4px;
  background: linear-gradient(90deg, transparent, #0071b9, transparent);
  border-radius: 2px;
}

.team-description {
  font-size: 22px;
  color: #666;
  line-height: 1.6;
  max-width: 600px;
  margin: 0 auto;
  font-weight: 300;
}

/* Filter Tabs */
.team-filters {
  display: flex;
  justify-content: center;
  gap: 10px;
  margin-bottom: 60px;
  flex-wrap: wrap;
}

.filter-btn {
  padding: 12px 24px;
  border: 2px solid #e9ecef;
  background: white;
  color: #666;
  border-radius: 50px;
  font-weight: 600;
  font-size: 14px;
  cursor: pointer;
  transition: all 0.3s ease;
  text-transform: uppercase;
  letter-spacing: 1px;
}

.filter-btn:hover,
.filter-btn.active {
  background: #0071b9;
  color: white;
  border-color: #0071b9;
  transform: translateY(-2px);
  box-shadow: 0 8px 25px rgba(0, 113, 185, 0.3);
}

/* Team Grid */
.team-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
  gap: 30px;
  max-width: 1200px;
  margin: 0 auto;
}

/* Team Card */
.team-card {
  position: relative;
  border-radius: 24px;
  overflow: hidden;
  transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
  cursor: pointer;
  background: white;
  box-shadow: 0 10px 40px rgba(0, 0, 0, 0.1);
  height: 420px;
}

.team-card:hover {
  transform: translateY(-12px) scale(1.02);
  box-shadow: 0 25px 60px rgba(0, 113, 185, 0.2);
}

/* Card Background */
.card-background {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 60%;
  border-radius: 24px 24px 80px 80px;
  transition: all 0.4s ease;
}

.card-blue { background: linear-gradient(135deg, #0071b9, #005a91); }
.card-orange { background: linear-gradient(135deg, #ff8b21, #e67e22); }
.card-purple { background: linear-gradient(135deg, #8e44ad, #9b59b6); }
.card-green { background: linear-gradient(135deg, #28a745, #27ae60); }
.card-red { background: linear-gradient(135deg, #dc3545, #e74c3c); }

/* Member Image Container */
.member-image-container {
  position: relative;
  width: 120px;
  height: 120px;
  margin: 40px auto 20px;
  border-radius: 50%;
  overflow: hidden;
  border: 4px solid white;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);
  transition: all 0.3s ease;
}

.team-card:hover .member-image-container {
  transform: scale(1.1);
}

.member-image,
.member-image-placeholder {
  width: 100%;
  height: 100%;
  object-fit: cover;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #f8f9fa;
  color: #ccc;
}

.member-image-placeholder svg {
  width: 60px;
  height: 60px;
}

/* Social Links */
.social-links {
  position: absolute;
  top: 20px;
  left: 20px;
  display: flex;
  gap: 8px;
  opacity: 0;
  transform: translateX(-20px);
  transition: all 0.3s ease;
}

.team-card:hover .social-links {
  opacity: 1;
  transform: translateX(0);
}

.social-link {
  width: 36px;
  height: 36px;
  background: rgba(255, 255, 255, 0.9);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #666;
  transition: all 0.3s ease;
  backdrop-filter: blur(10px);
}

.social-link:hover {
  background: white;
  transform: scale(1.1);
  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);
}

.social-link.linkedin:hover { color: #0077b5; }
.social-link.twitter:hover { color: #1da1f2; }
.social-link.email:hover { color: #ea4335; }
.social-link.website:hover { color: #0071b9; }

.social-link svg {
  width: 18px;
  height: 18px;
}

/* Info Badge */
.info-badge {
  position: absolute;
  top: 20px;
  left: 20px;
  width: 40px;
  height: 40px;
  background: rgba(255, 255, 255, 0.95);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #666;
  cursor: pointer;
  transition: all 0.3s ease;
  backdrop-filter: blur(10px);
  z-index: 3;
}

.info-badge:hover {
  background: white;
  transform: translateY(-2px);
  color: #0071b9;
  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.15);
}

.info-badge svg {
  width: 20px;
  height: 20px;
}

/* Member Content */
.member-content {
  padding: 0 30px 30px;
  text-align: center;
  position: relative;
  z-index: 2;
}

.member-name {
  font-size: 24px;
  font-weight: 700;
  color: #333;
  margin-bottom: 8px;
  transition: color 0.3s ease;
}

.member-position {
  font-size: 16px;
  color: #666;
  margin-bottom: 15px;
  font-weight: 500;
}

.member-bio {
  opacity: 0;
  max-height: 0;
  overflow: hidden;
  transition: all 0.3s ease;
}

.team-card:hover .member-bio {
  opacity: 1;
  max-height: 100px;
}

.member-bio p {
  font-size: 14px;
  color: #777;
  line-height: 1.5;
  margin: 0;
}

/* Empty State */
.empty-state {
  text-align: center;
  padding: 60px 20px;
  color: #666;
  font-size: 18px;
}

/* Floating Elements */
.floating-elements {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  pointer-events: none;
  overflow: hidden;
}

.floating-element {
  position: absolute;
  width: 6px;
  height: 6px;
  background: rgba(0, 113, 185, 0.3);
  border-radius: 50%;
  animation: float 8s ease-in-out infinite;
}

.floating-element.element-1 {
  top: 20%;
  left: 10%;
  animation-delay: 0s;
  animation-duration: 10s;
}

.floating-element.element-2 {
  top: 60%;
  right: 15%;
  animation-delay: 2s;
  animation-duration: 12s;
}

.floating-element.element-3 {
  bottom: 30%;
  left: 20%;
  animation-delay: 4s;
  animation-duration: 9s;
}

.floating-element.element-4 {
  bottom: 60%;
  right: 25%;
  animation-delay: 1s;
  animation-duration: 11s;
}

@keyframes float {
  0%, 100% {
    transform: translateY(0px) translateX(0px);
    opacity: 0.3;
  }
  25% {
    transform: translateY(-30px) translateX(15px);
    opacity: 1;
  }
  50% {
    transform: translateY(-15px) translateX(-20px);
    opacity: 0.6;
  }
  75% {
    transform: translateY(-35px) translateX(10px);
    opacity: 1;
  }
}

/* Filter Animation */
.team-card.filtering {
  opacity: 0;
  transform: scale(0.8);
  pointer-events: none;
}

.team-card.visible {
  opacity: 1;
  transform: scale(1);
  pointer-events: auto;
}

/* Mobile Responsive */
@media (max-width: 1024px) {
  .team-section {
    padding: 80px 0;
  }

  .team-title {
    font-size: 48px;
    letter-spacing: 2px;
  }

  .team-grid {
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 25px;
  }

  .team-card {
    height: 400px;
  }
}

@media (max-width: 768px) {
  .team-section {
    padding: 60px 0;
  }

  .team-container {
    padding: 0 15px;
  }

  .team-title {
    font-size: 36px;
    letter-spacing: 1px;
  }

  .team-description {
    font-size: 18px;
  }

  .team-header {
    margin-bottom: 60px;
  }

  .team-filters {
    margin-bottom: 40px;
  }

  .filter-btn {
    padding: 10px 20px;
    font-size: 12px;
  }

  .team-grid {
    grid-template-columns: 1fr;
    gap: 20px;
  }

  .team-card {
    height: 380px;
    margin: 0 auto;
    max-width: 350px;
  }

  .member-content {
    padding: 0 20px 20px;
  }

  .overlay-content {
    padding: 20px;
  }

  .overlay-content h4 {
    font-size: 24px;
  }

  .overlay-content p {
    font-size: 16px;
  }
}

@media (max-width: 480px) {
  .team-title {
    font-size: 28px;
  }

  .team-description {
    font-size: 16px;
  }

  .filter-btn {
    padding: 8px 16px;
    font-size: 11px;
  }

  .team-card {
    height: 360px;
  }

  .member-image-container {
    width: 100px;
    height: 100px;
    margin: 30px auto 15px;
  }

  .member-name {
    font-size: 20px;
  }

  .member-position {
    font-size: 14px;
  }
}

/* Animation on Scroll */
@media (prefers-reduced-motion: no-preference) {
  .team-card {
    opacity: 0;
    transform: translateY(30px);
    animation: fadeInUp 0.8s ease forwards;
  }

  .team-card:nth-child(1) { animation-delay: 0.1s; }
  .team-card:nth-child(2) { animation-delay: 0.2s; }
  .team-card:nth-child(3) { animation-delay: 0.3s; }
  .team-card:nth-child(4) { animation-delay: 0.4s; }
  .team-card:nth-child(5) { animation-delay: 0.5s; }
  .team-card:nth-child(6) { animation-delay: 0.6s; }

  @keyframes fadeInUp {
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }
}

/* Print Styles */
@media print {
  .team-section {
    background: white !important;
    color: black !important;
  }

  .floating-elements,
  .social-links,
  .card-overlay {
    display: none !important;
  }
}
</style>

<script>
// Team Section Functionality
document.addEventListener('DOMContentLoaded', function() {
  const filterBtns = document.querySelectorAll('.filter-btn');
  const teamCards = document.querySelectorAll('.team-card');
  const emptyState = document.querySelector('.empty-state');

  // Filter functionality
  filterBtns.forEach(btn => {
    btn.addEventListener('click', function() {
      const filter = this.getAttribute('data-filter');

      // Update active button
      filterBtns.forEach(b => b.classList.remove('active'));
      this.classList.add('active');

      // Filter cards
      let visibleCount = 0;
      teamCards.forEach(card => {
        const category = card.getAttribute('data-category');

        if (filter === 'all' || category === filter) {
          card.classList.remove('filtering');
          card.classList.add('visible');
          visibleCount++;
        } else {
          card.classList.add('filtering');
          card.classList.remove('visible');
        }
      });

      // Show/hide empty state
      if (emptyState) {
        emptyState.style.display = visibleCount === 0 ? 'block' : 'none';
      }
    });
  });

  // Initialize - show all cards
  teamCards.forEach(card => {
    card.classList.add('visible');
  });

  // Enhanced hover effects
  teamCards.forEach(card => {
    card.addEventListener('mouseenter', function() {
      // Add subtle glow effect
      this.style.boxShadow = '0 25px 60px rgba(0, 113, 185, 0.3)';
    });

    card.addEventListener('mouseleave', function() {
      // Remove glow effect
      this.style.boxShadow = '0 10px 40px rgba(0, 0, 0, 0.1)';
    });

    // Info badge click handler
    const infoBadge = card.querySelector('.info-badge');
    if (infoBadge) {
      infoBadge.addEventListener('click', function(e) {
        e.stopPropagation();

        // Toggle bio visibility
        const bio = card.querySelector('.member-bio');
        if (bio) {
          const isVisible = bio.style.opacity === '1';
          bio.style.opacity = isVisible ? '0' : '1';
          bio.style.maxHeight = isVisible ? '0' : '100px';
        }
      });
    }
  });

  // Intersection Observer for animations
  const observerOptions = {
    threshold: 0.1,
    rootMargin: '0px 0px -50px 0px'
  };

  const observer = new IntersectionObserver(function(entries) {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        entry.target.style.animationPlayState = 'running';
      }
    });
  }, observerOptions);

  teamCards.forEach(card => {
    observer.observe(card);
  });

  // Keyboard navigation
  teamCards.forEach(card => {
    card.setAttribute('tabindex', '0');

    card.addEventListener('keydown', function(e) {
      if (e.key === 'Enter' || e.key === ' ') {
        e.preventDefault();
        this.click();
      }
    });
  });

  // Touch/mobile enhancements
  teamCards.forEach(card => {
    let touchStartTime = 0;

    card.addEventListener('touchstart', function() {
      touchStartTime = Date.now();
    });

    card.addEventListener('touchend', function() {
      const touchDuration = Date.now() - touchStartTime;

      // Long press to show overlay on mobile
      if (touchDuration > 500) {
        const overlay = this.querySelector('.card-overlay');
        if (overlay) {
          overlay.style.opacity = overlay.style.opacity === '1' ? '0' : '1';
        }
      }
    });
  });
});

// Parallax effect for floating elements
window.addEventListener('scroll', function() {
  const scrolled = window.pageYOffset;
  const floatingElements = document.querySelectorAll('.floating-element');

  floatingElements.forEach((element, index) => {
    const rate = scrolled * (0.5 + index * 0.1);
    element.style.transform = `translate3d(0, ${rate}px, 0)`;
  });
});

// Performance optimization: throttle scroll events
function throttle(func, limit) {
  let inThrottle;
  return function() {
    const args = arguments;
    const context = this;
    if (!inThrottle) {
      func.apply(context, args);
      inThrottle = true;
      setTimeout(() => inThrottle = false, limit);
    }
  }
}

window.addEventListener('scroll', throttle(function() {
  // Parallax and scroll effects
}, 16));
</script>