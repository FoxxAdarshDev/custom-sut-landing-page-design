        function updateEventModal() {
            const modal = document.getElementById('tradeshow-event-modal');
            const image = document.getElementById('tradeshow-event-modal-image');
            const counter = document.getElementById('tradeshow-event-modal-counter');

            if (modal && image && counter) {
                const gallery = eventGalleries[currentEventGallery];
                if (gallery && gallery[currentEventIndex]) {
                    image.src = gallery[currentEventIndex];
                    counter.textContent = `${currentEventIndex + 1} of ${gallery.length}`;
                }
            }
        }

        function prevEventImage() {
            const gallery = eventGalleries[currentEventGallery];
            if (gallery) {
                currentEventIndex = (currentEventIndex - 1 + gallery.length) % gallery.length;
                updateEventModal();
            }
        }

        function nextEventImage() {
            const gallery = eventGalleries[currentEventGallery];
            if (gallery) {
                currentEventIndex = (currentEventIndex + 1) % gallery.length;
                updateEventModal();
            }
        }

        // Close modal when clicking outside
